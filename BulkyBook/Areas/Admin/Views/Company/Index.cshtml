@model IEnumerable<Company>
<div class="container-fluid">
    <div class="row  mt-5 mb-3">
        <div class="col-6">
            <h2 class="text-primary" > Company List </h2>
        </div>
        <div class="col-6 text-end">
            <a class="btn btn-info mt-2" asp-controller="Company" asp-action="Upsert"> 
                <i class="bi bi-plus-circle"></i> Add New Company 
            </a>
        </div>
    </div>
    <table id="tableData" class="table table-responsive table-striped table-bordered ">
        <thead>
            <tr>
                <th> Name </th>
                <th> StreetAddress </th>
                <th> City </th>
                <th> State </th>
                <th> Postal Code </th>
                <th> Phone Number </th>
                <th></th>
            </tr>
        </thead>
       
    </table>
</div>

@section Scripts
{
   <script src="~/js/company.js"></script>
}