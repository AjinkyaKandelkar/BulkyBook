@model IEnumerable<Product>
<div class="container-fluid">
    <div class="row  mt-5">
        <div class="col-6">
            <h2 class="text-primary" > Product List </h2>
        </div>
        <div class="col-6 text-end">
            <a class="btn btn-info" asp-controller="Product" asp-action="Upsert"> <i class="bi bi-plus-circle"></i> Add New Product </a>
        </div>
    </div>
    <table id="tableData" class="table table-responsive table-striped table-bordered ">
        <thead>
            <tr>
                <th> Title </th>
                <th> ISBN </th>
                <th> Price </th>
                <th> Author </th>
                <th> Category </th>
                <th> CoverType </th>
                <th> Hrllo </th>
            </tr>
        </thead>
       
    </table>
</div>

@section Scripts
{
   <script src="~/js/Example.js"></script>
}